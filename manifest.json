{
  "manifest_version": 2,
  "name": "Jsmond2016配置",
  "version": "1.0.0",
  "description": "Jsmond2016配置",
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "Jsmond2016配置",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "run_at": "document_end",
      "js": ["content.js"]
    }
  ],
  "options_page": "options.html",
  "permissions": [
    "contextMenus",
    "tabs",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [],
  "homepage_url": "https://github.com/Jsmond2016/openIn1s-chrome-plugin",
  "omnibox": {
    "keyword": "openIn1s"
  }
}