{
  "manifest_version": 2,
  "name": "openIn1S",
  "version": "1.0.0",
  "description": "在github1s打开本仓库",
  "icons": {
    "16": "img/favicon.png",
    "48": "img/favicon.png",
    "128": "img/favicon.png"
  },
  "browser_action": {
    "default_icon": "img/favicon.png",
    "default_title": "openIn1S"
  },
  "content_scripts": [{
      "matches": ["http://*/*", "https://*/*"],
      "js": ["js/index.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "contextMenus",
    "tabs",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [],
  "homepage_url": "https://github.com/Jsmond2016/openIn1s-chrome-plugin",
  "omnibox": {
    "keyword": "openIn1s"
  }
}